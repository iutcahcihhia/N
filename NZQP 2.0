void input_arrow_position(int type)
{
	int choice = 1;
	int input_menusize = sizeof(input_menu) / sizeof(input_menu[0]);
	while (true)
	{
		menu_input(choice, type);
		int arrow = _getch();
		if (arrow == 224) arrow = _getch();
		switch (arrow)
		{
		case _up:
		{
			choice--;
			if (choice == 0) choice = input_menusize;
			break;
		}
		case _down:
		{
			choice++;
			if (choice == input_menusize + 1) choice = 1;
			break;
		}
		case _enter:
		{
			switch (choice)
			{
			case 1: 
			{
				switch (type)
				{
				case 1: {delete[] number1; number1 = input_N(1, 0); break;}
				case 2: {delete[] number1; number1 = input_Z(1); break;}
				case 3: {delete_Q(fraction1); fraction1 = input_Q(1, 0, 0); break;}
				case 4: {delete_P(polynom1); polynom1 = input_P(1); break;}
				}
				break;
			}
			case 2:
			{
				switch (type)
				{
				case 1: {delete[] number2; number2 = input_N(2, 0); break;}
				case 2: {delete[] number2; number2 = input_Z(2); break;}
				case 3: {delete_Q(fraction2); fraction2 = input_Q(2, 0, 0); break;}
				case 4: {delete_P(polynom2); polynom2 = input_P(2); break;}
				}
				break;
			}
			case 3:
			{
				switch (type)
				{
				case 1: {delete[] number1; number1 = read_from_file_NZ(0); break;}
				case 2: {delete[] number1; number1 = read_from_file_NZ(1); break;}
				case 3: {delete_Q(fraction1); fraction1 = read_from_file_Q(); break;}
				case 4: {delete_P(polynom1); polynom1 = read_from_file_P(); break;}
				}
				break;
			}
			case 4:
			{
				switch (type)
				{
				case 1: {delete[] number2; number2 = read_from_file_NZ(0); break;}
				case 2: {delete[] number2; number2 = read_from_file_NZ(1); break;}
				case 3: {delete_Q(fraction2); fraction2 = read_from_file_Q(); break;}
				case 4: {delete_P(polynom2); polynom2 = read_from_file_P(); break;}
				}
				break;
			}
			case 5:
			{
				switch (type)
				{
				case 1: {if (number1 != NULL) write_to_file_NZ(number1, 0); else { cout << "Error! Number 1 doesn't exist."; Sleep(500); } break;}
				case 2: {if (number1 != NULL) write_to_file_NZ(number1, 1); else { cout << "Error! Number 1 doesn't exist."; Sleep(500); } break;}
				case 3: {if (fraction1 != NULL) write_to_file_Q(fraction1); else { cout << "Error! Fraction 1 doesn't exist."; Sleep(500); } break;}
				case 4: {if (polynom1 != NULL) write_to_file_P(polynom1); else { cout << "Error! Polynom 1 doesn't exist."; Sleep(500); } break;}
				}
				break;
			}
			case 6:
			{
				switch (type)
				{
				case 1: {if (number2 != NULL) write_to_file_NZ(number2, 0); else { cout << "Error! Number 2 doesn't exist."; Sleep(500); } break;}
				case 2: {if (number2 != NULL) write_to_file_NZ(number2, 1); else { cout << "Error! Number 2 doesn't exist."; Sleep(500); } break;}
				case 3: {if (fraction2 != NULL) write_to_file_Q(fraction2); else { cout << "Error! Fraction 2 doesn't exist."; Sleep(500); } break;}
				case 4: {if (polynom2 != NULL) write_to_file_P(polynom2); else { cout << "Error! Polynom 2 doesn't exist."; Sleep(500); } break;}
				}
				break;
			}
			case 7:
			{
				switch (type)
				{
				case 1: {if (number1 != NULL && number2 != NULL) return;}
				case 2: {if (number1 != NULL && number2 != NULL) return;}
				case 3: {if (fraction1 != NULL && fraction2 != NULL) return;}
				case 4: {if (polynom1 != NULL && polynom2 != NULL) return;}
				}
				break;
			}
			}
			break;
		}
		}
	}
}
void arrow_position(int menu_size, int type)
{
	bool input_flag = true;
	int choice = 1;
	while (true)
	{
		if (choice != 1)
		{
			switch (type)
			{
			case 1: {menu_N(choice, 1); break;}
			case 2: {menu_Z(choice, 1); break;}
			case 3: {menu_Q(choice, 1); break;}
			case 4: {menu_P(choice, 1); break;}
			}
		}
		if (choice == 1)
		{
			if (input_flag == true)
			{
				switch (type)
				{
				case 1: {menu_N(choice, 1); break;}
				case 2: {menu_Z(choice, 1); break;}
				case 3: {menu_Q(choice, 1); break;}
				case 4: {menu_P(choice, 1); break;}
				}
			}
			else
			{
				switch (type)
				{
				case 1:
				{
					menu_N(choice, 0);
					cout << "Number 1: ";
					num_output_NZ(number1, 0, 0, 1);
					cout << "Number 2: ";
					num_output_NZ(number2, 0, 0, 1);
					cout << "Press enter to input new numbers";
					break;
				}
				case 2:
				{
					menu_Z(choice, 0);
					cout << "Number 1: ";
					num_output_NZ(number1, 1, 0, 1);
					cout << "Number 2: ";
					num_output_NZ(number2, 1, 0, 1);
					cout << "Press enter to input new numbers";
					break;
				}

				case 3:
				{
					menu_Q(choice, 0);
					cout << "Fraction 1: ";
					if (INT_Q_B(fraction1) == 0) num_output_Q(fraction1, 1);
					else num_output_NZ(fraction1->numerator, 1, 0, 1);
					cout << "Fraction 2: ";
					if (INT_Q_B(fraction2) == 0) num_output_Q(fraction2, 1);
					else num_output_NZ(fraction2->numerator, 1, 0, 1);
					cout << "Press enter to input new fractions";
					break;
				}
				case 4:
				{
					menu_P(choice, 0);
					cout << "Polynom 1: ";
					num_output_P(polynom1, 1);
					cout << "Polynom 2: ";
					num_output_P(polynom2, 1);
					cout << "Press enter to input new polynoms";
					break;
				}
				}
			}
		}
		int arrow = _getch();
		if (arrow == 224) arrow = _getch();
		switch (arrow)
		{
		case _up:
		{
			choice--;
			if (choice == 0) choice = menu_size;
			switch (type)
			{
			case 1: {if (result_NZ != NULL) { delete[] result_NZ; result_NZ = NULL; } break; }
			case 2: {if (result_NZ != NULL) { delete[] result_NZ; result_NZ = NULL; } break;}
			case 3: {if (result_Q != NULL) { delete_Q(result_Q); result_Q = NULL; } break;}
			case 4:
			{
				if (result_P1 != NULL) { delete_P(result_P1); result_P1 = NULL; }
				if (result_P2 != NULL) { delete_P(result_P2); result_P2 = NULL; }
				break;
			}
			}
			break;
		}
		case _down:
		{
			choice++;
			if (choice == menu_size + 1) choice = 1;
			switch (type)
			{
			case 1: {if (result_NZ != NULL) { delete[] result_NZ; result_NZ = NULL; } break; }
			case 2: {if (result_NZ != NULL) { delete[] result_NZ; result_NZ = NULL; } break;}
			case 3: {if (result_Q != NULL) { delete_Q(result_Q); result_Q = NULL; } break;}
			case 4:
			{
				if (result_P1 != NULL) { delete_P(result_P1); result_P1 = NULL; }
				if (result_P2 != NULL) { delete_P(result_P2); result_P2 = NULL; }
				break;
			}
			}
			break;
		}
		case _enter:
		{
			if (choice == menu_size || choice == 1)
			{
				if (choice == menu_size)
				{
					switch (type)
					{
					case 1: {delete[] number1; delete[] number2; number1 = NULL; number2 = NULL; return;}
					case 2: {delete[] number1; delete[] number2; number1 = NULL; number2 = NULL; return;}
					case 3: {delete_Q(fraction1); delete_Q(fraction2); fraction1 = NULL; fraction2 = NULL; return;}
					case 4: {delete_P(polynom1); delete_P(polynom2); polynom1 = NULL; polynom2 = NULL; return;}
					}
				}
				if (choice == 1) input_arrow_position(type);
			}
			else
			{
				switch (type)
				{
				case 1: {if (result_NZ != NULL) write_to_file_NZ(result_NZ, 0); else { cout << "Error! Result doesn't exist."; Sleep(500); } break;}
				case 2: {if (result_NZ != NULL) write_to_file_NZ(result_NZ, 1); else { cout << "Error! Result doesn't exist."; Sleep(500); } break;}
				case 3: {if (result_Q != NULL) write_to_file_Q(result_Q); else { cout << "Error! Result doesn't exist."; Sleep(500); } break;}
				case 4: 
				{
					if (choice != 1 && choice != 8 && choice != 9 && choice != menusize_P) write_to_file_P(result_P1);
					if (choice == 8 || choice == 9)
					{
						int res = 0;
						cout << "Choose result (1 or 2, any number to skip): ";
						cin >> res;
						if (res == 1) write_to_file_P(result_P1);
						if (res == 2) write_to_file_P(result_P2);
					}
					break;
				}
				}
			}
			break;
		}
		}
		input_flag = false;
	}
}
void module_arrow_position()
{
	int choice = 1;
	int module_menusize = sizeof(module_menu) / sizeof(module_menu[0]);
	int menusize_N = sizeof(N_menu) / sizeof(N_menu[0]);
	int menusize_Z = sizeof(Z_menu) / sizeof(Z_menu[0]);
	int menusize_Q = sizeof(Q_menu) / sizeof(Q_menu[0]);
	int menusize_P = sizeof(P_menu) / sizeof(P_menu[0]);
	while (true)
	{
		menu_module(choice);
		int arrow = _getch();
		if (arrow == 224) arrow = _getch();
		switch (arrow)
		{
		case _up:
		{
			choice--;
			if (choice == 0) choice = module_menusize;
			break;
		}
		case _down:
		{
			choice++;
			if (choice == module_menusize + 1) choice = 1;
			break;
		}
		case _enter:
		{
			switch (choice)
			{
			case 1: {instruction(); break;}
			case 2: {arrow_position(menusize_N, 1); break;}
			case 3: {arrow_position(menusize_Z, 2); break;}
			case 4: {arrow_position(menusize_Q, 3); break;}
			case 5: {arrow_position(menusize_P, 4); break;}
			case 6: return;
			}

		}
		}
	}
}

int main()
{
	module_arrow_position();
}

