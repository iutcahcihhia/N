			delete[] p;
			p = NULL;
			maxl += 20;
		}
	}
	FR* result = RED_Q_Q(fraction);
	delete_Q(fraction);
	return result;
}
FR* input_P(int n)
{
	FR* pol_head = NULL;
	FR* polynom = NULL;
	int counter = 1;
	int len = 0;
	int maxl = 20;
	char num;
	int k = 0;
	while (true) //ввод коэффициентов и степеней многочленов
	{
		if (pol_head == NULL)
		{
			polynom = input_Q(n, counter, 1);
			polynom->degree = input_N(n, counter);
			pol_head = polynom;
		}
		else
		{
			polynom->next = input_Q(n, counter, 1);
			polynom = polynom->next;
